{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "import/no-extraneous-dependencies":"off",
        "no-console":"off"
    },
    "plugins": ["jasmine"]
}
